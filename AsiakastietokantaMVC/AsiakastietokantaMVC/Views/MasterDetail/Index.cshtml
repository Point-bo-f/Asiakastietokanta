@model IEnumerable<MasterDetail.Models.Henkilo>
@{
    ViewBag.Title = "List Henkilöt";
}

<h2>List Henkilöt</h2>
<table class="table table-striped">
    <thead>
        <tr>
            <th>HenkiloId</th>
            <th>Etunimi</th>
            <th>Sukunimi</th>
            <th>Osoite</th>
            <th>Esimies</th>
        </tr>
    </thead>
    <tbody>
        @foreach (MasterDetail.Models.Henkilot henk in Model)
        {
        <tr>
            <td>@henk.HenkiloID</td>
            <td>@henk.Etunimi</td>
            <td>@henk.Sukunimi</td>
            <td>@henk.Osoite</td>
            <td>@henk.Esimies</td>
       </tr>
        }
    </tbody>
</table>

